          {% capture name %}
          <p class="item__details--name">{{ product.name }}</p>

          {% endcapture %}
          <% } else { %>
        
            <p class="item__details--name">EYE COLOR PALETTE</p>
            <% } %>
  
        <p class="item__details--jpname">アイカラーパレット</p>
        <p class="item__details--price">¥2,420 <i>(tax in)</i></p>
         
      </div>
      <div class="item__details--icon">
        
        <img src="<%= imgpath %>/assets/images/uptorigin/icon_eye.png" alt="" width="" height="">
      </div>
    </div>

    <div class="item__details--inner aic">
      <ul class="item__details--color eye" id="itemDetails">
        <li class="item__details--colorlist active" data-name="01 amber more" data-masterid="20" onclick="click01();"></li>
        <li class="item__details--colorlist" data-name="02 melty mellow" data-masterid="21" onclick="click02();"></li>
        <li class="item__details--colorlist" data-name="03 teddy coco" data-masterid="22" onclick="click03();""></li>
