<section class="product__de">
  <h1 class="product__de--en">Vegan Vita Collection Limited Set</h1>
  <h2 class="product__de--jp">ヴィーガンビタコレクション 限定企画セット</h2>
 <h3 class="product__de--price"><p class="dashed">¥7,340</p>{{ product.price | money }}<span> ( inc.tax )</span></h3>  

  <!-- <h3 class="product__de--price">¥5,872<span> ( inc.tax )</span></h3> -->
</section>

<section class="rnCartbtn">
  <div class="rnCartbtn__inner">

      <div class="rnCartbtn__blc">
          <fieldset>
              <div class="rnCartbtn__radiobtn">
                  <input type="radio" id="01clear" name="powder"
                      data-url="https://perseperse.myshopify.com/cart/add?id=43771979530476&quantity=1"
                      value="01 CLEAR" checked />
                  <label for="01clear"></label>

                  <input type="radio" id="02beige" name="powder"
                      data-url="https://perseperse.myshopify.com/cart/add?id=43771979563244&quantity=1"
                      value="02 SKIN BEIGE" />
                  <label for="02beige"></label>


              </div>
          </fieldset>
          <p class="rnCartbtn__name" id="js-validastionname">01 CLEAR</p>

      </div>

      <div class="" id="vitasetBtn">
      {%- for variant in product.variants -%}
          {%- assign is_in_stock = false -%}
          {%- if 0 < variant.inventory_quantity -%}{%- assign is_in_stock = true -%}{%- endif -%}
          

          
          {%- if is_in_stock -%}
              <a class="product__cartbtn set{{ variant.id }}"
              href="https://perseperse.myshopify.com/cart/add?id={{ variant.id }}&quantity=1" id="js-cartlink">
              <p>BUY</p>
              <span class="product__cartbtn--img">cartimg<span>
              </a> 


          {%- elsif is_in_stock == false -%}
                  <div class="product__cartbtn--disable set{{ variant.id }}" style="overflow: hidden; display: block; margin-bottom :100px;">
                      売り切れ中
                  </div>
          {%- endif -%}
          {%- endfor -%}

      </div>
       

  {% comment %} <div class="product__cartbtn--disable"
      style="overflow: hidden; display: block; margin-bottom :100px;">
      COMING SOON <br>
      <span>2023.6.21(WED)12:00〜</span>
  </div> {% endcomment %}
</div>

</section>